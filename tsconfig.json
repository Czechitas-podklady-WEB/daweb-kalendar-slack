{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"lib": ["ES2022", "DOM"],
		"module": "ES2022",
		"target": "ES2022",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"jsx": "react",
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node"
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
